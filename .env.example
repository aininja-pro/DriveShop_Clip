# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Session Configuration
SESSION_TIMEOUT_HOURS=48  # Increased from 24 to reduce logout frequency
SESSION_CHECK_INTERVAL_MINUTES=10  # Only check session every 10 minutes

# Performance Configuration
CACHE_TTL_SECONDS=600  # Cache data for 10 minutes
ENABLE_LAZY_LOADING=true
AGGRID_PAGE_SIZE=100  # Number of rows per page in AgGrid

# Other API Keys
OPENAI_API_KEY=your_openai_key
SLACK_WEBHOOK_URL=your_slack_webhook